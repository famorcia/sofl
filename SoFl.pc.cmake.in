prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
bindir=@CMAKE_INSTALL_FULL_BINDIR@
libdir=@CMAKE_INSTALL_FULL_LIBDIR@
includedir=@CMAKE_INSTALL_FULL_INCLUDEDIR@
datarootdir=@CMAKE_INSTALL_FULL_DATAROOTDIR@
datadir=@CMAKE_INSTALL_FULL_DATADIR@
docdir=@CMAKE_INSTALL_FULL_DOCDIR@
infodir=@CMAKE_INSTALL_FULL_INFODIR@
mandir=@CMAKE_INSTALL_FULL_MANDIR@
htmldir=${docdir}/html

Name: @PROJECT_NAME@
Description: @PROJECT_DESCRIPTION@
Version: @PROJECT_VERSION@
Requires: @SOFL_PKG_DEPS@
Conflicts:
Libs: -L${libdir} @SOFL_PKG_LIBS@
Cflags: -I${includedir} @SOFL_PKG_FLAGS@

frameworkdir=@frameworkdir@
fltk_version=@fltk_VERSION_MAJOR@.@fltk_VERSION_MINOR@.@fltk_VERSION_PATCH@
